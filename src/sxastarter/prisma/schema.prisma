generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model CustomerEnquiry {
  CustomerEnquiryID Int @id(map: "PK__Customer__8D4F8F10C7391710") @unique(map: "UQ__Customer__8D4F8F112B12568E") @default(autoincrement())

  @@ignore
}

model LoginDetails {
  UserId      Int         @id(map: "PK__LoginDet__1788CC4C8B3260E4")
  Username    String      @db.VarChar(100)
  Password    String      @db.VarChar(50)
  UserDetails UserDetails @relation(fields: [UserId], references: [UserId], onUpdate: NoAction, map: "FK__LoginDeta__UserI__76969D2E")
}

model UserDetails {
  UserId       Int           @id(map: "PK__UserDeta__1788CC4C4C60C617") @default(autoincrement())
  Title        String        @db.VarChar(5)
  FirstName    String        @db.VarChar(255)
  LastName     String        @db.VarChar(255)
  Gender       String        @db.VarChar(10)
  Email        String        @unique(map: "UQ__UserDeta__A9D105344A206566") @db.VarChar(50)
  Dob          DateTime      @db.Date
  MobileNumber String        @unique(map: "UQ__UserDeta__250375B13EBE4FE2") @db.VarChar(15)
  CreatedAt    DateTime      @default(now(), map: "DF__UserDetai__Creat__73BA3083") @db.DateTime
  UpdatedAt    DateTime?     @db.DateTime
  LoginDetails LoginDetails?
}

model Enquiry {
  EnquiryID      Int       @id(map: "PK__Enquiry__0A019B9D81509497") @default(autoincrement())
  FirstName      String    @db.VarChar(50)
  LastName       String?   @db.VarChar(50)
  Email          String?   @db.VarChar(255)
  NumberOfGuest  String?   @db.VarChar(2)
  RoomType       String?   @db.VarChar(50)
  FreePickup     String?   @db.VarChar(50)
  ArrivalDate    DateTime? @db.Date
  DepartureDate  DateTime? @db.Date
  SpecialRequest String?   @db.VarChar(255)
}

model MallEnquiry {
  EnquiryID      Int      @id(map: "PK__MallEnqu__0A019B9DA734489A") @default(autoincrement())
  FirstName      String   @db.VarChar(50)
  LastName       String?  @db.VarChar(50)
  Email          String?  @db.VarChar(255)
  EnquiryType    String?  @db.VarChar(255)
  EnquiryDetails String?  @db.VarChar(255)
  CreatedAt      DateTime @db.DateTime
}
